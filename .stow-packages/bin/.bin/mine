#!/bin/bash
#Generated by JetBrains Toolbox 1.28.1.15219 at 2023-09-17T21:28:25.322047

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/quynguyen/Library/Application Support/JetBrains/Toolbox/apps/RubyMine/ch-0/232.9559.58/RubyMine.app/Contents/MacOS/rubymine" $wait --args "${ideargs[@]}"